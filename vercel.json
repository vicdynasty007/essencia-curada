{
  "redirects": [
    {
      "source": "http://(.*)",
      "destination": "https://essenciacurada.com/$1",
      "permanent": true
    },
    {
      "source": "https://www.essenciacurada.com/(.*)",
      "destination": "https://essenciacurada.com/$1",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Strict-Transport-Security", "value": "max-age=31536000; includeSubDomains; preload" }
      ]
    }
  ]
}
